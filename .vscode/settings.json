  "[json]": {
    "editor.formatOnSave": true
  },
  "eslint.validate": [
    "vue",
    "html",
    "javascript",
    "json",
    "typescript"
  ],
  "vue3snippets.enable-compile-vue-file-on-did-save-code": false,
 }